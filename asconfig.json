{
  "targets": {
    "release": {
      "outFile": "build/contract.wasm",
      "textFile": "build/contract.wat",
      "sourceMap": false,
      "optimize": true,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": false
    },
    "debug": {
      "outFile": "build/contract_debug.wasm",
      "textFile": "build/contract_debug.wat",
      "sourceMap": true,
      "debug": true
    }
  },
  "options": {
    "bindings": "esm",
    "runtime": "stub",
    "exportRuntime": false
  }
}
